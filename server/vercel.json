{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",  
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server.js" 
    }
  ],
  "headers": [
    { "source": "/api/socket", "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }] }
  ]
}
