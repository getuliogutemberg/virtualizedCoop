{
    "version": 2,
    "builds": [
      {
        "src": "./dist/index.js", 
        "use": "@now/node"
      }
    ],
    "routes": [
      { 
        "src": "/(.*)", 
        "dest": "./dist/index.js",
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
     }
    ]
  }
  